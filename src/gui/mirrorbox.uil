/************************* Definition der opt. Element Box  *******************/

object
    EOElementBox  : XmFormDialog {                   
        arguments {
            XmNdialogTitle      = "Optical Element Box";
            XmNdialogStyle      = XmDIALOG_MODELESS;
            XmNnoResize         = true;
            XmNdefaultPosition  = false;
            XmNx                = 0;
            XmNy                = 600;
            XmNautoUnmanage     = false;
            XmNallowOverlap     = false;
            XmNdefaultButton    = XmPushButton EOEOK;
            XmNcancelButton     = XmPushButton EOECancel;
            XmNhorizontalSpacing = 10;
            XmNverticalSpacing  = 20;
            XmNnavigationType   = XmEXCLUSIVE_TAB_GROUP;
            };
            controls {
            XmForm              EOEInputForm;
            XmSeparator         EOESeparator; 
            XmPushButton        EOEOK;
            XmPushButton        EOEApply;
            XmPushButton        EOEDefault;
            XmPushButton        EOECancel;
            XmSeparator         EOESeparator1; 
            };          
       callbacks { MrmNcreateCallback = procedure create_proc(kEOElementBox);};
        };   

object
    EOEInputForm : XmForm {
        arguments {
            XmNshadowType       = XmSHADOW_IN;
            XmNshadowThickness  = 3;
            XmNtopAttachment    = XmATTACH_FORM;
            XmNtopOffset        = 10; 
            XmNleftAttachment   = XmATTACH_FORM;
            XmNrightAttachment  = XmATTACH_FORM;
            XmNrightOffset      = 10; 
            XmNallowOverlap     = false;
            XmNnavigationType   = XmEXCLUSIVE_TAB_GROUP;
            XmNforeground       = color('white'); 
            XmNbackground       = color('blue');      
                         };
        controls {
             XmOptionMenu        EOOptMenu; 
	     XmRadioBox          EORadBox;	 
             XmSeparator         EOESeparatorM;
             XmLabel             CFFHeaderLabel;
             XmLabel             CFFHeader1Label;
             XmRowColumn         EOERCFF;
	     XmRowColumn         EOERRad;
             XmSeparator         EOESeparatorN;
             XmRowColumn         EOERGR;
             XmSeparator         EOESeparatorO;
             XmRowColumn         EOERAli;
	    ! XmRowColumn         EOERC;
             XmToggleButton      EGNITranslation1;
	     XmLabel             EOEInputLabel;
             XmLabel             GratingLabel;
             XmLabel             AliLabel;
		};
        };

object
    EGNITranslation1 : XmToggleButton {
        arguments {
            XmNlabelString      = "translate grating (NIM)";
            XmNbottomAttachment    = XmATTACH_WIDGET; 
