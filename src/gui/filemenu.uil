object
    FPrintFileBox  : XmFormDialog {   
	   arguments {
		XmNdialogTitle	    = "Print File Box (max. 5 files)";
		XmNautoUnmanage     = false; 
                XmNdefaultButton    = XmPushButton FPFOK;
                XmNcancelButton     = XmPushButton FPFCancel;
                XmNhorizontalSpacing= 10; 
                XmNverticalSpacing  = 15; 
		};
        controls {
                XmScrolledList  FPFList;
                XmPushButton    FPFAdd; 
                XmPushButton    FPFDel; 
                XmPushButton    FPFOK;
                XmPushButton    FPFCancel;
		};          
        callbacks { MrmNcreateCallback = procedure create_proc(kFPFBox);};
        };    

