! The file pulldown menu with the push buttons it controls.

object
    file_menu : XmPulldownMenu {
        controls {
            XmPushButton        FPrintButton;                  
            XmPushButton        FFileButton;   
            XmPushButton        FSaveAsButton;      
            XmPushButton        FExitButton;
            };
        callbacks {
            MrmNcreateCallback  = procedure create_proc   (kFileMenu);
            XmNhelpCallback     = procedure sens_help_proc(k_file_help);  
            };
        };         

object                      
    FPrintButton : XmPushButton {
        arguments {
            XmNlabelString      = "Print...";                         
            XmNmnemonic         = keysym("P");
            XmNsensitive        = false;
	};        
        callbacks {      
            XmNactivateCallback = procedure activate_proc (kFPrint);
            XmNhelpCallback     = procedure sens_help_proc(k_print_help);   
            };               
        };            
                 
object                      
    FFileButton : XmPushButton {
        arguments {
            XmNlabelString      = "Files...";                         
            XmNmnemonic         = keysym("F");
            };
        callbacks {      
            XmNactivateCallback = procedure activate_proc (kFFile);
            XmNhelpCallback     = procedure sens_help_proc(k_print_help);   
            };               
        };            

object                      
    FSaveAsButton : XmPushButton {
        arguments {
            XmNlabelString      = "Save as...";                         
            XmNmnemonic         = keysym("S");
            };
        callbacks {      
            XmNactivateCallback = procedure activate_proc (kFSaveAsButton);
            XmNhelpCallback     = procedure sens_help_proc(k_print_help);   
            };               
        };            

object
    FExitButton : XmPushButton {
        arguments {
            XmNlabelString      = "Exit";                            
            XmNaccelerator      = "Ctrl<Key>z:";
            XmNacceleratorText  = "Crtl/Z";   
            XmNmnemonic         = keysym("E");
            };
        callbacks {
            XmNactivateCallback = procedure exit_proc('normal exit');
            };
        };                                  


