object                               !* Motif Widget 
    FileSelectionDialog : XmFileSelectionDialog {                   
        arguments {
            XmNdialogTitle       = "File Selection Box";   
            XmNautoUnmanage      = true;
            };     
      callbacks {
	MrmNcreateCallback = procedure create_proc (kFileSelectionDialog);
        XmNokCallback      = procedure FileSelectionProc(kFileSelectionOk); 
        XmNcancelCallback  = procedure FileSelectionProc(kFileSelectionCancel); 
        XmNhelpCallback    = procedure sens_help_proc(k_print_help);  
		};
        };       

object
    FFileBox : XmFormDialog {                   
